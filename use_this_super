import java.util.*;
class one{
int a;
public int geta(){
    return a;
}
one(int a){
    this.a =a;

}
        }
        class two extends one{
       two(int c){
           super(c);
           System.out.println("i am constructor");
    }
        }
public class use_this {
    public static void main(String[] args) {
        one e = new one(6);
        two d = new two(2);
        System.out.println(e.geta());
    }
}
