import java.util.*;
public class binary_to_decimal
{
public static void main(String args[]) 
 {
    int n,ans=0,i=0;
    Scanner sc=new Scanner(System.in);
    n=sc.nextInt();
    while(n!=0)
    {
        int digit=n%10;
        if(digit==1)
        {
        ans= (int) ((Math.pow(2,i))+ ans);
        }
        
        n=n/10;
        i++;
    }
    System.out.println(ans);
 }
}
