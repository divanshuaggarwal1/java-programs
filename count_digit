import java.util.*;
public class count_number {
    public static void main(String[] args) {
        int num=0;
        System.out.println("enter number ");
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
      while(n>0){
         n=n/10;
          num++;
   }
        System.out.println("number of digit is-> "+ num);
    }
}
